{
  "name" : "oneclicksolar/oneclicksolar",
  "description" : "Yii 2 Basic Project Template",
  "require" : {
    "php" : ">=5.4.0",
    "yiisoft/yii2" : "2.0.11.1",
    "yiisoft/yii2-bootstrap" : "2.0.6",
    "yiisoft/yii2-swiftmailer" : "*",
    "dektrium/yii2-user" : "0.9.*@dev",
    "mdmsoft/yii2-admin" : "2.0.*",
    "bower-asset/bootstrap" : "3.3.*",
    "bower-asset/admin-lte" : "2.3.11",
    "bower-asset/punycode" : "1.3.2",
    "bower-asset/jquery" : "^1.12",
    "sersid/yii2-font-awesome-asset" : "*",
    "bower-asset/fancybox" : "*",
    "bower-asset/datetimepicker" : "*",
    "mbostock/d3" : "^3.5",
    "rmrevin/yii2-minify-view" : "~1.11",
    "yiisoft/yii2-jui" : "^2.0",
    "janisto/yii2-timepicker" : "^1.1",
    "2amigos/yii2-selectize-widget" : "~1.0",
    "2amigos/yii2-ckeditor-widget" : "~1.0",
    "mihaildev/yii2-elfinder" : "*",
    "zxbodya/yii2-image-attachment" : "*@dev",
    "zxbodya/yii2-gallery-manager" : "*@dev",
    "pheme/yii2-jui-timepicker" : "*",
    "himiklab/yii2-recaptcha-widget" : "*",
    "twilio/sdk": "5.4.1",
    // "sprytechies/yii2-nocaptcha-recaptcha": "dev-master"
  },
  "require-dev" : {
    "yiisoft/yii2-debug" : "*",
    "yiisoft/yii2-gii" : "*"
  },
  "license" : "BSD-3-Clause",
  "keywords" : [ "yii2", "framework", "basic", "project template" ],
  "extra" : {
    "yii\\composer\\Installer::postCreateProject" : {
      "setPermission" : [ {
        "runtime" : "0777",
        "web/assets" : "0777",
        "yii" : "0755"
      } ],
      "generateCookieValidationKey" : [ "config/web.php" ]
    },
    "asset-installer-paths" : {
      "npm-asset-library" : "vendor/npm",
      "bower-asset-library" : "vendor/bower"
    }
  },
  "minimum-stability" : "dev",
  "type" : "project",
  "scripts" : {
    "post-create-project-cmd" : [ "yii\\composer\\Installer::postCreateProject" ]
  },
  "support" : {
    "issues" : "https://github.com/yiisoft/yii2/issues?state=open",
    "forum" : "http://www.yiiframework.com/forum/",
    "wiki" : "http://www.yiiframework.com/wiki/",
    "irc" : "irc://irc.freenode.net/yii",
    "source" : "https://github.com/yiisoft/yii2"
  },
  "config" : {
    "process-timeout" : 1800
  },
  "homepage" : "http://www.yiiframework.com/",
  "prefer-stable" : true
}
